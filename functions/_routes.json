{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/images/*",
    "/*.ico",
    "/*.svg",
    "/*.png"
  ],
  "routes": [
    {
      "pattern": "/api/url",
      "script": "api/url.js"
    },
    {
      "pattern": "/api/tempMail/domains",
      "script": "api/tempMail.js"
    },
    {
      "pattern": "/*",
      "script": "[[path]].js"
    }
  ]
} 